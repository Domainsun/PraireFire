<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffe5e5"
    tools:context="com.praire.fire.merchant.SettledActivity1">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y96"
        android:background="@mipmap/head_backgroud">

        <TextView
            android:id="@+id/tv_back"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/y60"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:gravity="center"
            android:layout_centerVertical="true"
            android:drawablePadding="@dimen/x20"
            android:drawableRight="@mipmap/back" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:text="商家入驻申请"
            android:textColor="@android:color/white"
            android:textSize="20sp" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        android:background="@color/white"
        android:layout_marginBottom="@dimen/y2">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="汽车"
            android:layout_marginTop="@dimen/y33"
            android:layout_marginLeft="@dimen/x30"
            android:textColor="@color/text_color"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="@dimen/y30"
            android:layout_height="@dimen/y30"
            android:layout_marginTop="@dimen/y30"
            android:layout_marginRight="@dimen/x30"
            android:src="@mipmap/store_type_selected"
            android:layout_alignParentRight="true"/>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        android:background="@color/white"
        android:layout_marginBottom="@dimen/y2">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="教育"
            android:layout_marginTop="@dimen/y33"
            android:layout_marginLeft="@dimen/x30"
            android:textColor="@color/text_color"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv2"
            android:layout_width="@dimen/y30"
            android:layout_height="@dimen/y30"
            android:layout_marginTop="@dimen/y30"
            android:layout_marginRight="@dimen/x30"
            android:src="@mipmap/store_type_normal"
            android:layout_alignParentRight="true"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        android:background="@color/white"
        android:layout_marginBottom="@dimen/y2">
        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="休闲"
            android:layout_marginTop="@dimen/y33"
            android:layout_marginLeft="@dimen/x30"
            android:textColor="@color/text_color"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv3"
            android:layout_width="@dimen/y30"
            android:layout_height="@dimen/y30"
            android:layout_marginTop="@dimen/y30"
            android:layout_marginRight="@dimen/x30"
            android:src="@mipmap/store_type_normal"
            android:layout_alignParentRight="true"/>

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        android:background="@color/white"
        android:layout_marginBottom="@dimen/y2">
        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="旅游"
            android:layout_marginTop="@dimen/y33"
            android:layout_marginLeft="@dimen/x30"
            android:textColor="@color/text_color"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv4"
            android:layout_width="@dimen/y30"
            android:layout_height="@dimen/y30"
            android:layout_marginTop="@dimen/y30"
            android:layout_marginRight="@dimen/x30"
            android:src="@mipmap/store_type_normal"
            android:layout_alignParentRight="true"/>

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        android:background="@color/white"
        android:layout_marginBottom="@dimen/y2">
        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="服饰"
            android:layout_marginTop="@dimen/y33"
            android:layout_marginLeft="@dimen/x30"
            android:textColor="@color/text_color"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv5"
            android:layout_width="@dimen/y30"
            android:layout_height="@dimen/y30"
            android:layout_marginTop="@dimen/y30"
            android:layout_marginRight="@dimen/x30"
            android:src="@mipmap/store_type_normal"
            android:layout_alignParentRight="true"/>

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y100"
        android:background="@color/white"
        android:layout_marginBottom="@dimen/y2">
        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="其他"
            android:layout_marginTop="@dimen/y33"
            android:layout_marginLeft="@dimen/x30"
            android:textColor="@color/text_color"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv6"
            android:layout_width="@dimen/y30"
            android:layout_height="@dimen/y30"
            android:layout_marginTop="@dimen/y30"
            android:layout_marginRight="@dimen/x30"
            android:src="@mipmap/store_type_normal"
            android:layout_alignParentRight="true"/>

    </RelativeLayout>



</LinearLayout>
